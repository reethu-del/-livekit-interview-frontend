# ============================================
# REACT FRONTEND ENVIRONMENT VARIABLES
# ============================================
# Copy this file to .env in the react-frontend directory

# ============================================
# REQUIRED - Backend API URL
# ============================================
# URL of the Python backend API server
VITE_API_URL=http://localhost:8000
# OR: VITE_BACKEND_URL=http://localhost:8000

# ============================================
# REQUIRED - Frontend App URL
# ============================================
# Used by backend for generating interview links
VITE_APP_URL=http://localhost:3000

# ============================================
# OPTIONAL - LiveKit Sandbox Configuration
# ============================================
# Only needed if using LiveKit Cloud Sandbox
VITE_CONN_DETAILS_ENDPOINT=
VITE_APP_CONFIG_ENDPOINT=
VITE_SANDBOX_ID=
